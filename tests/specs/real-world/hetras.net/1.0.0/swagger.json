    "link_object": {
      "description": "A Link Object represents a hyperlink from the containing resource to a URI.",
      "properties": {
        "Deprecation": {
          "description": "The \"deprecation\" property is OPTIONAL.Â Its presence indicates that the link is to be deprecated (i.e.\r\n            removed) at a future date.  Its value is a URL that SHOULD provide\r\n            further information about the deprecation.\r\n            A client SHOULD provide some notification (for example, by logging a\r\n            warning message) whenever it traverses over a link that has this\r\n            property.  The notification SHOULD include the deprecation property's\r\n            value so that a client maintainer can easily find information about\r\n            the deprecation.",
          "type": "string"
        },
        "HrefLang": {
          "description": "The \"hreflang\" property is OPTIONAL.Â Its value is a string and is intended for indicating the language of\r\n            the target resource (as defined by [RFC5988]).",
          "type": "string"
        },
        "Name": {
          "description": "The \"name\" property is OPTIONAL.Â Its value MAY be used as a secondary key for selecting Link Objects\r\n            which share the same relation type.",
          "type": "string"
        },
        "Profile": {
          "description": "The \"profile\" property is OPTIONAL.Â Its value is a string which is a URI that hints about the profile.",
          "type": "string"
        },
        "Templated": {
          "description": "The \"templated\" property is OPTIONAL.Â Its value is boolean and SHOULD be true when the Link Object's \"href\"\r\n            property is a URI Template.\r\n            Its value SHOULD be considered false if it is undefined or any other\r\n            value than true.",
          "type": "boolean"
        },
        "Title": {
          "description": "The \"title\" property is OPTIONAL.Â Its value is a string and is intended for labelling the link with a\r\n            human-readable identifier (as defined by [RFC5988]).",
          "type": "string"
        },
        "Type": {
          "description": "The \"type\" property is OPTIONAL.Â Its value is a string used as a hint to indicate the media type\r\n            expected when dereferencing the target resource.",
          "type": "string"
        },
        "Uri": {
          "description": "The \"href\" property is REQUIRED.Â Its value is either a URI [RFC3986] or a URI Template [RFC6570].\r\n            If the value is a URI Template then the Link Object SHOULD have a\r\n            \"templated\" attribute whose value is true.",
          "type": "string"
        }
      },
      "required": [
        "Uri"
      ],
      "type": "object"
    },
