{
  "name": "swagger-parser",
  "version": "1.0.1",
  "description": "Parses a Swagger spec (in JSON or YAML format), validates it against the Swagger schema, and dereferences all $ref pointers",
  "keywords": [
    "json",
    "swagger",
    "spec",
    "specification",
    "schema",
    "validate",
    "validation",
    "reference",
    "dereference"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/BigstickCarpet/swagger-parser",
  "main": "index.js",
  "scripts": {
    "build": "gulp build",
    "prepublish": "gulp prepublish",
    "test": "mocha --ui bdd --recursive tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigstickCarpet/swagger-parser.git"
  },
  "dependencies": {
    "debug": "~2.0.0",
    "js-yaml": "^3.2.2",
    "lodash": "^2.4.1",
    "swagger-schema-official": "^2.0.0-a33091a",
    "tv4": "^1.1.0"
  },
  "devDependencies": {
    "browserify": "^6.3.3",
    "chai": "^1.10.0",
    "gulp": "^3.8.10",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-uglify": "^1.0.1",
    "jquery": "^2.1.1",
    "karma": "^0.12.28",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.5",
    "karma-firefox-launcher": "^0.1.3",
    "karma-ie-launcher": "^0.1.5",
    "karma-mocha": "^0.1.9",
    "karma-mocha-reporter": "^0.3.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.4",
    "mocha": "^2.0.1",
    "sinon": "^1.12.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
